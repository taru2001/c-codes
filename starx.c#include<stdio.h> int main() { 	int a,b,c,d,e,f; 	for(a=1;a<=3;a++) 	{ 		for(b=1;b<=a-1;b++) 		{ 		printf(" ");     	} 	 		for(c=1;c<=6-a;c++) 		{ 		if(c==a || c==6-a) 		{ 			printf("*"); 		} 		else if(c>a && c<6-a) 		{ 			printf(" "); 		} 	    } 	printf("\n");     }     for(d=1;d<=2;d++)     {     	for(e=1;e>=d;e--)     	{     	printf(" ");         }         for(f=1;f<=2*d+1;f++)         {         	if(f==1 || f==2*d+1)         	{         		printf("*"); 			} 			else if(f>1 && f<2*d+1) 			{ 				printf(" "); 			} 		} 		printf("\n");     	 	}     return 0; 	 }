#include<stdio.h>
int main()
{
	int a,b,c,d,e,f;
	for(a=1;a<=3;a++)
	{
		for(b=1;b<=a-1;b++)
		{
		printf(" ");
    	}
	
		for(c=1;c<=6-a;c++)
		{
		if(c==a || c==6-a)
		{
			printf("*");
		}
		else if(c>a && c<6-a)
		{
			printf(" ");
		}
	    }
	printf("\n");
    }
    for(d=1;d<=2;d++)
    {
    	for(e=1;e>=d;e--)
    	{
    	printf(" ");
        }
        for(f=1;f<=2*d+1;f++)
        {
        	if(f==1 || f==2*d+1)
        	{
        		printf("*");
			}
			else if(f>1 && f<2*d+1)
			{
				printf(" ");
			}
		}
		printf("\n");
    	
	}
    return 0;
	
}
